<form action = "/random/results">
  <div>
    <label for = "d">Minimum</label>
    <input id = "d" name = "user_min" placeholder="E.g. 1.5">
  </div>

  <div>
    <label for = "e">Maximum</label>
    <input id = "e" name = "user_max" placeholder="E.g. 4.5">
  </div>

  <div>
    <button>Pick random number</button>
  </div>
</form>
